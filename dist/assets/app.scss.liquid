		top:initial;
		left:initial;
		transform:initial;
	}
	.ProductCustom__right__step--2.active .ProductCustom__text-container--right{
		position:relative;
		max-width:50px;
		height:100%;
		background-color:transparent;
		border-left:1px solid #EBEBEB;
