	.ProductCustom__swatch--color{
		margin:0 auto;
	}
	.ProductCustom__text-container--right p,
	.ProductCustom__text-container--right h1{
		display:none;
	}
	.ProductCustom--mobile{
		width:100%;
		display:block;
	}
	.ProductCustom__size .hidden {
		display:flex;
	}
	.ProductCustom__size .hidden .select-wrapper{
		width:49%;
		margin-right:1%;
